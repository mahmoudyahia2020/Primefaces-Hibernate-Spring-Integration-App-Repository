<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	<navigation-rule>
		<navigation-case>
			<from-outcome>meetingPoints</from-outcome>
			<to-view-id>meetingPoints.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>meetings</from-outcome>
			<to-view-id>meetings.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>registerMembers</from-outcome>
			<to-view-id>registerMembers.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>meetingMembers</from-outcome>
			<to-view-id>meetingMembers.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>